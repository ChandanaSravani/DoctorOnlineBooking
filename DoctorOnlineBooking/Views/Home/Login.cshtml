@model DoctorOnlineBooking.Models.Login
@{
    ViewBag.Title = "Login";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Login</h2>
<div class="container col-md-12">
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div class="form-group">
            <label>UserMail/SapId</label>
            @Html.TextBoxFor(c => c.UserName,new { @class = "form-control", autocomplete = "off" })
            @Html.ValidationMessageFor(c=>c.UserName)
        </div>
        <div class="form-group">
            @Html.LabelFor(c => c.Password)
            @Html.TextBoxFor(c => c.Password, new { @class = "form-control" })
            @Html.ValidationMessageFor(c=>c.Password)
        </div>
    <div class="form-group">
        <input type="submit" value="Login" id="btnSubmit" class="btn btn-outline-primary" />
    </div>
}
    </div>

